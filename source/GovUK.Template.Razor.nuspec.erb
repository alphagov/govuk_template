<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
    <metadata>
        <id>GovUK.Template.Razor</id>
        <version><%= GovukTemplate::VERSION %></version>
        <authors>Government Digital Service developers</authors>
        <requireLicenseAcceptance>false</requireLicenseAcceptance>
		<licenseUrl>https://github.com/alphagov/govuk_template/blob/master/LICENCE.txt</licenseUrl>
		<projectUrl>https://github.com/alphagov/govuk_template</projectUrl>
        <description>Razor template packaged version of the GOV.UK template</description>
		<tags>GOV.UK ASP.NET Razor Template</tags>
    </metadata>
	<files>
		<file src="assets/images/**" target="content/Content/govuk/images/" />
		<file src="assets/stylesheets/**" target="content/Content/govuk/stylesheets/" />
		<file src="assets/javascripts/**" target="content/Scripts/govuk/" />
		<file src="views/layouts/govuk_template.cshtml" target="content/Views/Shared/_Layout.GovUK.cshtml" />
	</files>
</package>
